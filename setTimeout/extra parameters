<input type="button" value="get new Date() in 2 seconds">
<p></p>
<script>
 document.querySelector("input").onclick = function() {
  setTimeout( myFunction, 2000, "parameter 1", "parameter 2");
 }
 function myFunction(param1, param2) {
  document.querySelector("p").innerHTML = new Date() + param1 + param2;
 }
</script>
