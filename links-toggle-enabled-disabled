<script>
var a = "";
var aHref = "";
function myFunction(e) {
 if (a==e) {
  a.href="javascript: void(0)";
  a.target="_self";
} 
else {
 a.href=aHref;
 a.target="_blank";
 a=e;
 aHref=e.href;
}

}
</script>
